{
  "version": 3,
  "builds": [
    {
      "src": "package.json",
      "use": "@frontity/now"
    }
  ]
}
{
    "headers": [
        {
            "source": "/(.*)",
            "headers" : [
                {
                    "key" : "Cache-Control",
                    "value" : "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}
